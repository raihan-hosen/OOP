#include <iostream>
using namespace std;
 
int findRoot(int x)
{
    while (x % 2 == 0)
        x /= 2;
    return x;
}
 
int main()
{
    int t;
    cin >> t;
 
    while (t--)
    {
        int n;
        cin >> n;
 
        int a[n + 1];
 
        for (int i = 1; i <= n; i++)
            cin >> a[i];
 
        bool possible = true;
 
        for (int i = 1; i <= n; i++)
        {
            if (findRoot(i) != findRoot(a[i]))
            {
                possible = false;
                break;
            }
        }
 
        if (possible)
            cout << "YES\n";
        else
            cout << "NO\n";
    }
 
    return 0;
}
