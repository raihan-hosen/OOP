#include <iostream>
#include <iomanip>
using namespace std;

int main()
{
    double M[12][12];
    int C;
    char ch;
    cin >> C >> ch;
    for (int i = 0; i < 12; i++)
    {
        for (int j = 0; j < 12; j++)
        {
            cin >> M[i][j];
        }
    }
    double sum = 0.0;
    for (int i = 0; i < 12; i++)
    {
        sum += M[i][C];
    }
    if (ch == 'S')
    {
        cout << fixed << setprecision(1) << sum << endl;
    }
    else if (ch == 'M')
    {
        cout << fixed << setprecision(1) << sum / 12.0 << endl;
    }
    return 0;
}
