#include <iostream>
#include <iomanip>
using namespace std;

double sum(double M[12][12])
{
    double total = 0;
    for (int i = 7; i < 12; i++)
        for (int j = 12 - i; j < i; j++)
            total += M[i][j];
    return total;
}

double average(double M[12][12])
{
    double total = 0;
    int count = 0;
    for (int i = 7; i < 12; i++)
        for (int j = 12 - i; j < i; j++)
        {
            total += M[i][j];
            count++;
        }
    return total / count;
}

int main()
{
    char op;
    double M[12][12];
    cin >> op;
    for (int i = 0; i < 12; i++)
        for (int j = 0; j < 12; j++)
            cin >> M[i][j];
    if (op == 'S')
        cout << fixed << setprecision(1) << sum(M) << endl;
    else if (op == 'M')
        cout << fixed << setprecision(1) << average(M) << endl;
    return 0;
}
