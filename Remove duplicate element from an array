#include <iostream>
using namespace std;

int main()
{
    int arr[] = {1, 2, 2, 3, 4, 4, 5};
    int n = sizeof(arr) / sizeof(arr[0]);

    int result[100];
    int size = 0;

    for (int i = 0; i < n; i++)
    {
        bool duplicate = false;
        for (int j = 0; j < size; j++)
        {
            if (arr[i] == result[j])
            {
                duplicate = true;
                break;
            }
        }
        if (!duplicate)
        {
            result[size++] = arr[i];
        }
    }

    for (int i = 0; i < size; i++)
    {
        cout << result[i] << " ";
    }
    return 0;
}
