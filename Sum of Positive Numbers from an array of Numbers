#include <iostream>
using namespace std;
int main()
{
    int N, int number[N];
    cout << "Enter the number of integers: ";
    cin >> N;
    
    for (int i = 0; i < N; i++)
    {
        cout << "Give Integer Number" << " " << i + 1 << ": ";
        cin >> number[i];
    }

    int sum = 0;
    for (int i = 0; i < N; i++)
    {
        if (number[i] > 0)
            sum += number[i];
    }
    cout << "Sum of Positive Numbers: " << sum << endl;
    return 0;
}
